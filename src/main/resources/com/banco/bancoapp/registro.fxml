<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" stylesheets="css/styles.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.banco.bancoapp.controllers.RegistroController">
    <Label text="NIF" />
    <TextField fx:id="nifTextField" />

    <Label text="Nombre" />
    <TextField fx:id="nombreTextField" />

    <Label text="Apellidos" />
    <TextField fx:id="apellidosTextField" />

    <Label text="Año de nacimiento" />
    <TextField fx:id="añoNacimientoTextField" />

   <Label layoutX="10.0" layoutY="136.0" text="Direccion" />
   <TextField fx:id="direccionTextField" layoutX="10.0" layoutY="153.0" />

   <Label layoutX="10.0" layoutY="178.0" text="E-mail" />
   <TextField fx:id="emailTextField" layoutX="10.0" layoutY="195.0" />

   <Label layoutX="10.0" layoutY="245.0" text="Telefono" />
   <TextField fx:id="telefonoTextField" layoutX="10.0" layoutY="262.0" />

    <Button contentDisplay="CENTER" onAction="#crearUsuario" text="Crear usuario">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Button>
   <Button layoutX="255.0" layoutY="357.0" mnemonicParsing="false" onAction="#volver" text="Volver" />
</VBox>
