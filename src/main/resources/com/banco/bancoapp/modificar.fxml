<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.banco.bancoapp.controllers.ModificarUsuarioController">
    <Label alignment="CENTER" contentDisplay="CENTER" text="NIF">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="nifTextField" />
    <Label text="Nombre">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="nombreTextField" />
    <Label text="Apellidos">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="apellidosTextField" />
    <Label text="Año de Nacimiento">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="añoNacimientoTextField" />
    <Label text="Dirección">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="direccionTextField" />
    <Label text="Email">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="emailTextField" />
    <Label text="Teléfono">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="telefonoTextField" />

    <Button onAction="#guardarCambios" text="Guardar cambios" >
      <VBox.margin>
         <Insets left="250.0" top="10.0" />
      </VBox.margin></Button>
</VBox>
